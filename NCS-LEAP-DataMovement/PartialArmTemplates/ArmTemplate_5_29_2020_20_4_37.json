								}
							},
							"sink": {
								"type": "BinarySink",
								"storeSettings": {
									"type": "AzureBlobStorageWriteSettings"
								}
							},
							"enableStaging": false,
							"dataIntegrationUnits": 2
						},
						"inputs": [
							{
								"referenceName": "S3LeapDataSet_Cloud2",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "LeapStorageSourceDataset",
								"type": "DatasetReference"
							}
						]
					},
					{
						"name": "LeapCSV_Cloud3_Staging",
