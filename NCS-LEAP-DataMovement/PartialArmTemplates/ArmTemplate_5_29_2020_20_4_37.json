						],
						"outputs": [
							{
								"referenceName": "LeapStorageSourceDataset",
								"type": "DatasetReference"
							}
						]
					},
					{
						"name": "LeapCSV_Cloud2_Staging",
						"type": "Copy",
						"dependsOn": [
							{
								"activity": "DeleteFiles_Cloud1",
